{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["ES2017", "DOM"],
    "types": ["@cloudflare/workers-types/2023-07-01", "vite/client"],
    "resolveJsonModule": true,
    "moduleResolution": "Bundler",
    "checkJs": false,
    "skipLibCheck": true,
    "exactOptionalPropertyTypes": false,

    "declaration": true,
    "declarationMap": true
  },
  "include": ["./index.ts"]
}
